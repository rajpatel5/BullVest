        <shape android:shape="rectangle"  >
            <corners android:radius="14dip" />
            <stroke android:width="3dp" android:color="@color/tradeOrderButton"/>
            <solid android:color="@color/colorPrimary"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="14dip" />
            <solid android:color="@color/colorPrimary"/>
        </shape>
    </item>
</selector>